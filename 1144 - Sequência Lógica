#include <stdio.h>
#include <math.h>

main (void){
    int n, n2, saida, i, j;
    scanf("%d", &n);
    for(i=0;i<=n;i++){
        if( i > 0){
        printf("%d %d %d\n", i, i*i, i*i*i);
        printf("%d %d %d\n", i, (i*i)+1, (i*i*i)+1);
        }
    }
    
}
