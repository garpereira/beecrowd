//https://www.urionlinejudge.com.br/judge/pt/profile/347554
#include <stdio.h>

main (void){
    int valor_x, i;
    do{
        scanf("%d", &valor_x);
        for(i=1;i<valor_x;i++){
            printf("%d ", i);
        }
        if( i == valor_x){
                printf("%d\n", valor_x);
            }
    }
    while(valor_x != 0);
}
