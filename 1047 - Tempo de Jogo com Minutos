#include <stdio.h>

int main(){
    int hinicio, minicio, hfim, mfim;
    scanf("%d %d %d %d", &hinicio, &minicio, &hfim, &mfim);
    if(hinicio == hfim && minicio == mfim){
      printf("O JOGO DUROU 24 HORA(S) E 0 MINUTO(S)\n");
      return 0;
    }
    int i, aux = 0;
    for(i = 0; i< 1440;i++){
        if(hinicio == hfim && minicio == mfim){
            printf("O JOGO DUROU %d HORA(S) E %d MINUTO(S)\n", i/60, aux);
            break;
        }
        minicio+= 1;
        aux +=1;
        if(aux == 60)
          aux = 0;
        if(minicio == 60){
            hinicio +=1;
            minicio = 0;
        }
        if(hinicio == 24)
          hinicio = 0;
    }
    return 0;
}
